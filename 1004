    {

        Console.WriteLine("숫자를 입력하세요: ");




        int num = int.Parse(Console.ReadLine());
        //int num = Convert.ToInt32(Console.ReadLine());

        for (int i = 0; i < num; i++)
        {



            for (int j = 0; j <= i; j++)
            {

                Console.Write("*");



            }

            Console.WriteLine();




        }
    }

}

===============================================================================



























































































































































































































































































































