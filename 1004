using System;



class Program




{
    static void Main()
    {

        Console.WriteLine("숫자를 입력하세요: ");




        int N = int.Parse(Console.ReadLine());

        for (int i = 1; i <= 11; i++)
        {



            for (int j = 1; j <= i; j++)
            {

                Console.Write("*");



            }

            Console.WriteLine();




        }
    }

}


