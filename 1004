using System;
//&,| ^ ~ ! >> << 
    class Program
{ 
    static void Main()
    {

        byte x = 0b1010; //byte를 받는다는 의미 0b
        byte y = 0b1100; // 12
        Console.WriteLine(x + y);
        Console.WriteLine(y);
        Console.WriteLine(x);    


    }
        




}

