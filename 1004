switch 구문


        Console.WriteLine("가장! 좋아하는 프로그램 언어를 선택하세요");
        Console.Write("1. C#\t2.C++\t3.Python\t4.JAVA");
        
        int input_num = Convert.ToInt32(Console.ReadLine());
        // 숫자로 입력받는거
        switch (input_num)
        {
            case 1:
                {
                    Console.WriteLine("선택하신 언어는 C# 입니다.");

                    break;

                }
            case 2:
                {
                    Console.WriteLine("선택하신 언어는 C++입니다.");
                    break;
                }


             case 3:
                        {
                            Console.WriteLine("선택하신 언어는 파이썬입니다.");

                            break;
                        }
                    case 4:
                        {
                            Console.WriteLine("선택하신 언어는 자바입니다.");
                            break;
                        }

                    default:
                        {
                            Console.WriteLine("항목에 없는 입력입니다.");
                            break;
                        }





                    }

