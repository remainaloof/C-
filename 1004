    class Program
{
    static void Main()
    {
        Console.WriteLine();
        string? input = Console.ReadLine(); 
        var outcome = int.TryParse(input, out var result);
        Console.Write($"{input} {outcome}");
        
        if ( outcome)
        {
            Console.WriteLine("정상 입력입니다");
        }
        else
        {
            Console.WriteLine("다시 입력하세요");
             Console.WriteLine("프로그램을 종료합니다");
        }
       
    }



