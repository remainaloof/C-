using System;



class Program

    


{
    static void Main()
    {
        int[,] a =
        {

            { 10, 20, 30
            },
            { 40, 50, 60,
            }



        };

        Console.WriteLine($"a[0, 0] = {a[0,0]}");
        Console.WriteLine($"a[0, 1] = {a[0, 1]}");
        Console.WriteLine($"a[0, 2] = {a[0, 2]}");



        Console.WriteLine($"a[1, 0] = {a[1, 0]}");
        Console.WriteLine($"a[1, 1] = {a[1, 1]}");
        Console.WriteLine($"a[1, 2] = {a[1, 2]}");

    }

}

===================> 
a[0, 0] = 10
a[0, 1] = 20
a[0, 2] = 30
a[1, 0] = 40
a[1, 1] = 50
a[1, 2] = 60



