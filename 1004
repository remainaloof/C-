using System;
//조건문
    class Program
{
    static void Main()
    {

        Console.Writ{ "성적을 입력하세요:__\b\b"};
        string? input = Console.ReadLine();
        int number = Convert.ToInt32(input);

        if (number >= 90) Console.WriteLine("su");
        else if (number >= 80) Console.WriteLine("woo");
        else if (number >= 70) Console.WriteLine("mi");
        else if (number >= 60) Console.WriteLine("yang");
        else Console.WriteLine("ga");

    }



}
