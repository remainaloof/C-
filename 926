 double pi = 3.14;
            double area;
            Console.WriteLine("원의 반지름을 입력하세요, (cm)");
            string? input = Console.ReadLine();
            double radius = Convert.ToDouble(input);
            area = pi * radius * radius;

            Console.WriteLine($"원의 반지름: {radius}");
            Console.WriteLine($"원의 넓이는 {area:0.000} cm^2");


