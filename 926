bool x, y;
            x = 10 > 0;
            y = 10 < 0;

            Console.WriteLine($"x && y = {x && y}");
            Console.WriteLine($"x || y = { x || y}");
            Console.WriteLine($" !x = {!x}");



x && y = False
x || y = True
 !x = False

------------------------------------------------------------------------


a Í∞Ä 100Ïù¥ ÏïÑÎãàÍ±∞ÎÇò
