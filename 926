-------------------------------------------------------------------------------------------------------------------------------

 int a = 10;
            int b = 20;
           
            Console.WriteLine($"a>b = {a > b}");
            
 
=>    a>b = False
------------------------------------------------------------------------------------------------------------------------------

// a >= b
// a <= b
// a == b
// a != b 같지 않다!
// a = b b를 a에 대입해라! 그다음 a값을 반환해라!
-------------------------------------------------------------------------------------------------------------------------------

string right = "정답", wrong = "오답";
            bool value = true;
            string answer;

            answer = value ? right : wrong; // 삼(3)항 연산자 정답이 트루이면 right 을 반환하고 그렇지 않으면 wrong을 반환해라
            Console.WriteLine(answer);

            =>
            정답
----------------------------------------------------------------------------------------------------------------------------------



int a = 1, b = 2;
            
            string right = "정답", wrong = "오답";
            bool value = a > b;
            string answer;

            answer = value ? right : wrong; // 삼(3)항 연산자 정답이 트루이면 right 을 반환하고 그렇지 않으면 wrong을 반환해라
            Console.WriteLine(answer);

=> 오답

