int a = 1;
            Console.WriteLine($"a ={a}");
            Console.WriteLine($"++a = {++a}"); //Prefix
      
            Console.WriteLine($"a++ = {a++}"); //Postfix
            Console.WriteLine($"a = {a}"); //Prefix




            ==>

 a =1
++a = 2
a++ = 2
a = 3
-------------------------------------------------------------------------------------------------------------------------------------------




int a = 1;
            Console.WriteLine($"a ={a}");
            Console.WriteLine($"++a = {++a}"); //Prefix
      
            Console.WriteLine($"a++ = {a++}"); //Postfix
            Console.WriteLine($"a = {a}"); //Prefix

            int b = 2;

            int sum = a+b;
            Console.WriteLine($"a + b = {a + ++b}");
            Console.WriteLine($"b = {b}");
            =>
 a =1
++a = 2
a++ = 2
a = 3
a + b = 6
b = 3
-----------------------------------------------------------------------------------------------------------------------------------------


            int a = 1;
            Console.WriteLine($"a ={a}");
            Console.WriteLine($"++a = {++a}"); //Prefix
      
            Console.WriteLine($"a++ = {a++}"); //Postfix
            Console.WriteLine($"a = {a}"); //Prefix

            int b = 2;

            int sum = a+b;
            Console.WriteLine($"a + b = {a + b++}");
            Console.WriteLine($"b ={b}");
        }

















a =1
++a = 2
a++ = 2
a = 3
a + b = 5
b =3
-----------------------------------------------------------------------------------------------



그냥 int b = 2 + 1;
 이렇게 직관적으로 쓰는 것이 좋 음

 -------------------------------------------------------------------------------------------------






int b = 2;

            Console.WriteLine($"b = {b}");
            Console.WriteLine($"--b = {--b}");
            Console.WriteLine($"b-- = {b--}");
            Console.WriteLine($"b = {b}");



===========>

b = 2
--b = 1
b-- = 1
b = 0
-------------------------------------------------------------------------------------------------
 int a = 8;
            int b = 0, c = 0, d = 0, e = 0;

            b = a++; // b = 8 , a=9
            c = ++a;// c =10 a=10
            d = a--;// d=10 a=9
            e = --a;// e=8  a=8
