// 필요한 데이터를 원하는 용도에 맞게 형변환 하는 것이 중요한것

-----------------------------------------------------------------------------------------------------------------

int a = 2;
            int b = 3;
            int c = 5;

            Console.WriteLine($"a + b = {a + b}");
            Console.WriteLine($"a - b = {a - b}");
            Console.WriteLine($"a x b = {a * b}");
            Console.WriteLine($"a / b = {a / b}");    // 몫 반환 
            Console.WriteLine($"a % b = {a % b}");  // 나머지 값 반환





a + b = 5
a - b = -1
a x b = 6
a / b = 0
a % b = 2


==================================================================

Console.WriteLine($"a ={a}, b ={b}, c={c}");
            Console.WriteLine($"a + b = {a + b}");
            Console.WriteLine($"a - b = {a - b}");
            Console.WriteLine($"a x b = {a * b}");
            Console.WriteLine($"a / b = {a / b}");    // 몫 반환 
            Console.WriteLine($"a % b = {a % b}");  // 나머지 값 반환

            Console.WriteLine($"(a + b)*c = {(a + b)*c}");
            
a =2, b =3, c=5
a + b = 5
a - b = -1
a x b = 6
a / b = 0
a % b = 2
(a + b)*c = 25

