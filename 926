절대값이 출력 되도록 할 때



 

          


         
            Console.WriteLine("숫자를 입력하세요 : ");
            
       
            int number = Convert.ToInt32(Console.ReadLine());

            var answer = (number>0) ? number : -number;

          


            Console.WriteLine($"{number}의 절대값은 {answer} 입니댜");

-------------------------------------------------------------------



bool x, y;
            x = 10 > 0;
            y = 10 < 0;

            Console.WriteLine($"x && y = {x && y}");
            Console.WriteLine($"x || y = { x || y}");
            Console.WriteLine($" !x = {!x}");



x && y = False
x || y = True
 !x = False

------------------------------------------------------------------------


a 가 100이 아니거나
b 가 200이 아닌 경우 참인 경우



            bool a, b;
            a = 100 > 0;
            a = 100 < 0;
            b = 200 < 0;
            b = 200 > 0;

           
            Console.WriteLine($"x || y = { a || b }");
            




++

int a = 0;, b =0; //초기화 , initialization

// bool answer = (a != 100) || (b != 200);
bool answer = !((a == 100) && (b == 200));






bool result = answer ? true : false;
Console.WriteLine($"(a != 100) || (b != 200) = {result}");

