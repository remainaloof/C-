Console.WriteLine($"a ={a}, b ={b}, c={c}");
            Console.WriteLine($"a + b = {a + b}");
            Console.WriteLine($"a - b = {a - b}");
            Console.WriteLine($"a x b = {a * b}");
            Console.WriteLine($"a / b = {a / b}");    // Î™´ Î∞òÌôò 
            Console.WriteLine($"a % b = {a % b}");  // ÎÇòÎ®∏ÏßÄ Í∞í Î∞òÌôò

            Console.WriteLine($"(a + b)*c = {(a + b)*c}");
            
a =2, b =3, c=5
a + b = 5
a - b = -1
a x b = 6
a / b = 0
a % b = 2
(a + b)*c = 25

