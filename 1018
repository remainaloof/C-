    using System;

    public struct Point
    {
        public int X;
        public int Y;

        
    }

 class struct
 {
static void Main(string[]arg)
{

    Point point;
    point.X = 1;
    point.Y=2;

    Console.WriteLine($"x={point.X}, y = {point.Y}");
    Console.WriteLine($"x={x}, y = {y}");
}


 }




























































































