using System;
using.Collestions.Generic;
using System.Linq;
using.Text;
using System.Threading.Tasks;

namespace MathExample
{

class Progran
{
// Math class 활용하여 바로 적용하기.

  static void Main (string[]arg);
  {
          for(double arg = 0; ang<360; ,arg += 10 )
          {
              Console.CursorLeft = 40+ (int)(Math.Cos(arg*Math.PI/180)*20);
              Console.CursorTop = 12 +_ (int)(Math.Sin(arg*Math.PI/180)*10);
              Console.Write("*");
          }
          //1+e의 i+승 = 0
          //자연상수 e



  }



}
}


