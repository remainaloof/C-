< math api >

using System;

class MyClass
{


static int MyPow(int x, int p)
{

    int sum = 1;
    for (int i = 0; i < p; i++)
   {
      sum = sum * x;
   }
}



static void Main(string [] args)
{


int ret = MyPow(1,2);
// 1을 2번 곱한것 1의 2승

//  int ret = MyPow(2,8);
Console.WriteLine(ret);
Console.WriteLine(Math.Pow(2,16)); //api
}




}
