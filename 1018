메소드 오버로드


using System;


class MethodOverload
{



static void Main ()
{
    static void GetNumber(int number) => Console.WriteLine($"Int32: {number}");
     static void GetNumber(long number) => Console.WriteLine($"Int64: {number}");
}

static void Main()
{
    GetNumber(1018);
    GetNumber(1018L);
}




}

---------------------------






static void Multi() => Console.WriteLine(message);
static void Multi(string message, int count) => Console.WriteLine(message,count);

static void Multi(string message int count)
{
    for(int i = 0; i< count; i++)
    {
        Console.WriteLine(message);
    }
}

