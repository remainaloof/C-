class Review
{
    
    static void ShowMessage(string message)
    {
        Console.WriteLine(message);
    }



    static void Main(string[]args)
    {
       ShowMessage("좋은 아침");
    }
//쇼 메시지 옆에 단어가 위에 콘솔 라이트라인 메시지로 넘어감;


}
