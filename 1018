using System;

class MyClass
{

static void Main(strin[] args);

{
if(args.Length == 0)
{
    Console.WriteLine("입력된 인수가 없습니다.");
}
else
{

  foreach(var arg in args)
  {
    Console.WriteLine($"입력된 인수의 이름은 {args}입니다.");

  }

}    


}


}
