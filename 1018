class Review



{


    static double CircleArea(double r double pi = 3.14)
    {
        double a = r * r*pi;
        return a;
    }
static void Main(string[] args)
{

int val = CircleArea(3);
Console.WriteLine(val);
// Console.WriteLine($"원의 넓이 = {val}");

}



}
