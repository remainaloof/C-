static void Main()
{
int a = 0;
for ( int i = 0; i < 5; i++)
{

a = i;
Console.WriteLine($"a={a}");
}

Console.WriteLine($"a={a}");



}
=======================================
 CLass 에 대한 내용 공부할 것 !

int a = 0;

while ( a<6)
{

Console.WriteLine(a);
a++;

}
---------------------------------------------

int a = 0;

while(true)
{
Console.WriteLine(a);
a++;

if(a >= 5)
{

break;
}


}
Console.WriteLine("While문이 정지 되었습니다.");

}
}

