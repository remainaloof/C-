{

    public string? Nmae { get; set; } = "인생c#";
    public int Version { get; set; } = 8;
     

}


    class PropertyPractice
    {

        static void Main()
        {
        Publish publish = new Publish();
        Console.Write(publish.Nmae);
        Console.WriteLine(publish.Version);


    
































































































































































